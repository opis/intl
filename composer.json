{
    "name": "opis/intl",
    "description": "Internationalization library",
    "keywords": [
        "intl",
        "i18n",
        "language",
        "internationalization"
    ],
    "homepage": "https://www.opis.io/intl",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        },
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Opis\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\I18n\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}
